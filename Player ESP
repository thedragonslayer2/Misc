local a={enabled=false,health=false,distance=false,disname=false,Color=Color3.new(1,0,0)}local function b()for c,d in next,game:GetService"Players":GetChildren()do pcall(function()if game:GetService"Players".LocalPlayer~=d and d.Character then if d.Character:FindFirstChild"Head"and not d.Character.Head:FindFirstChild"NameEsp"and a.enabled then local e=Instance.new("BillboardGui",d.Character.Head)e.Name="NameEsp"e.Size=UDim2.new(1,200,2,75)e.Adornee=d.Character.Head;e.AlwaysOnTop=true;local f=Instance.new("TextLabel",e)f.TextWrapped=true;f.Font="ArialBold"f.Size=UDim2.new(1,0,1,0)f.TextSize=12;f.TextTransparency=0.2;f.TextStrokeTransparency=0.5;f.TextYAlignment="Top"f.TextColor3=a.Color;f.BackgroundTransparency=1 elseif a.enabled and d.Character.Head:FindFirstChild"NameEsp"then pcall(function()d.Character.Head.NameEsp.Enabled=true;d.Character.Head.NameEsp.TextLabel.Visible=true;d.Character.Head.NameEsp.TextLabel.TextColor3=a.Color;d.Character.Humanoid.NameDisplayDistance=0 end)if a.disname and d.DisplayName~=d.Name then local g=d.DisplayName.."\n"..d.Name;if a.distance then g=g.."\n"..(game:GetService"Players".LocalPlayer.Character.Head.Position-d.Character.Head.Position).Magnitude.."studs"else g="\n"..g end;if a.health then g=g.."\nHealth: "..math.ceil(d.Character.Humanoid.Health)else g="\n"..g end;d.Character.Head.NameEsp.TextLabel.Text=g else local g=d.Name;if a.distance then g=g.."\n"..(game:GetService"Players".LocalPlayer.Character.Head.Position-d.Character.Head.Position).Magnitude.."studs"else g="\n"..g end;if a.health then g=g.."\nHealth: "..math.ceil(d.Character.Humanoid.Health)else g="\n"..g end;d.Character.Head.NameEsp.TextLabel.Text=g end elseif not a.enabled and d.Character.Head:FindFirstChild"NameEsp"then d.Character.Head.NameEsp.TextLabel.Visible=false end end end)end end;spawn(function()while wait()do pcall(b)end end)return a
