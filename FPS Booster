repeat wait()until game:IsLoaded()local a,b,c,d,e,f={Disabled=true},workspace:WaitForChild"Terrain",game:GetService"Lighting",{},{},getgenv().sethiddenproperty;if hookmetamethod then local g;g=hookmetamethod(game,"__newindex",function(h,i,j)local k=d[h]and d[h][i]if k and(not a.Disabled or k.CantDisable)then if checkcaller and not checkcaller()then k.OldValue=j end;j=k.Value end;return g(h,i,j)end)end;function a:DontChange(k,l)e[k]=l end;function a:LockProperty(k,l,j,m)if e[k]==l then return end;if not d[k]then d[k]={}end;d[k][l]={Value=j,OldValue=k[l],CantDisable=m}if not a.Disabled or m then k[l]=j end end;function a:Boost()a.Disabled=nil;if f then f(b,"Decoration",false)end;if a.Done then for k,Data in next,d do k[Data.Property]=Data.Value end;return else a.Done=true;do local function n(o,p)if o:IsA"BasePart"then a:LockProperty(o,"Material","Plastic")a:LockProperty(o,"Reflectance",0)elseif o:IsA"ForceField"or o:IsA"Explosion"then a:LockProperty(o,"Visible",false)elseif o:IsA"Fire"or o:IsA"ParticleEmitter"or o:IsA"Trail"or o:IsA"SpotLight"or o:IsA"PointLight"or o:IsA"Smoke"or o:IsA"Sparkles"or o:IsA"Beam"then a:LockProperty(o,"Enabled",false)end end;workspace.DescendantAdded:Connect(n)for q,o in next,workspace:GetDescendants()do n(o)end end;do a:LockProperty(b,"WaterWaveSize",0)a:LockProperty(b,"WaterWaveSpeed",0)a:LockProperty(b,"WaterReflectance",0)a:LockProperty(b,"WaterTransparency",0)end;do local function n(o,r)if not r then task.wait()end;if string.find(o.ClassName,"Effect")then a:LockProperty(o,"Enabled",false)elseif o:IsA"Atmosphere"then a:LockProperty(o,"Density",0)end end;c.ChildAdded:Connect(n)for q,o in next,c:GetChildren()do n(o,true)end;a:LockProperty(c,"GlobalShadows",false)a:LockProperty(c,"FogEnd",9e9)end end end;function a:Disable()a.Disabled=true;for k,s in next,d do if not Data.CantDisable then for l,Data in next,s do k[l]=Data.OldValue end end end;if f then f(b,"Decoration",true)end end;return a
