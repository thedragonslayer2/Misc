repeat wait()until game:IsLoaded()local a,b,c,d,e={Enabled=false},workspace:WaitForChild"Terrain",game:GetService"Lighting",{},getgenv().sethiddenproperty;local f=getgenv().gethiddenproperty and getgenv().gethiddenproperty(b,"Decoration")a.Check=function(g,h)if not h then task.wait(0.1)end;if g:IsA"BasePart"then task.defer(a.LockProperty,a,g,"Material","Plastic")task.defer(a.LockProperty,a,g,"Reflectance",0)elseif g:IsA"ForceField"or g:IsA"Explosion"then task.defer(a.LockProperty,a,g,"Visible",false)elseif g:IsA"Atmosphere"then task.defer(a.LockProperty,a,g,"Density",0)elseif g:IsA"Fire"or g:IsA"ParticleEmitter"or g:IsA"Trail"or g:IsA"SpotLight"or g:IsA"PointLight"or g:IsA"Smoke"or g:IsA"Sparkles"or g:IsA"Beam"or string.find(g.ClassName,"Effect")then task.defer(a.LockProperty,a,g,"Enabled",false)end end;function a:RemoveObj(i,j)d[i]=j end;function a:LockProperty(i,j,k)if d[i]==j or not a.Enabled then return end;local l,m=i[j]m=i:GetPropertyChangedSignal(j):Connect(function()if i[j]~=k and(typeof(i[j])~="EnumItem"or i[j].Name~=k)then l=i[j]end;if a.Enabled then i[j]=k end end)i[j]=k;repeat task.wait()until not a.Enabled or d[i]==j;i[j],l,m=l end;function a:Toggle(n)a.Enabled=n;if not n then return end;if e and f then e(b,"Decoration",not n)end;do for o,g in next,workspace:GetDescendants()do a.Check(g,true)end;do a:LockProperty(b,"WaterWaveSize",0)a:LockProperty(b,"WaterWaveSpeed",0)a:LockProperty(b,"WaterReflectance",0)a:LockProperty(b,"WaterTransparency",0)end end;do for o,g in next,c:GetChildren()do a.Check(g,true)end;a:LockProperty(c,"GlobalShadows",false)a:LockProperty(c,"FogEnd",9e9)end end;workspace.DescendantAdded:Connect(a.Check)c.ChildAdded:Connect(a.Check)return a
