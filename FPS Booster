repeat wait()until game:IsLoaded()local a,b,c,d,e,f,g={Enabled=false},workspace:WaitForChild"Terrain",game:GetService"Lighting",{},{},getgenv().sethiddenproperty,false;local h=getgenv().gethiddenproperty and getgenv().gethiddenproperty(b,"Decoration")if hookmetamethod then local i;i=hookmetamethod(game,"__newindex",function(j,k,l)local m=d[j]and d[j][k]if m and(a.Enabled or m.CantDisable)then if checkcaller and not checkcaller()then m.OldValue=l end;l=m.Value end;return i(j,k,l)end)end;function a:DontChange(n,o)e[n]=o end;function a:LockProperty(n,o,l,p)if e[n]==o then return end;if not d[n]then d[n]={}end;d[n][o]={Value=l,OldValue=n[o],CantDisable=p}if a.Enabled or p then n[o]=l end end;function a:RemoveObj(n)for n,q in next,d[n]or{}do for o,m in next,q do n[o]=m.OldValue end end end;function a:Toggle(r)a.Enabled=r end;LooP(function()local s=a.Enabled;if g~=s then if f and h then f(b,"Decoration",not s)end;if s then if a.Check then for n,q in next,d do for o,m in next,q do n[o]=m.Value end end else a.Check=function(t,u)if not u then task.wait()end;if t:IsA"BasePart"then a:LockProperty(t,"Material","Plastic")a:LockProperty(t,"Reflectance",0)elseif t:IsA"ForceField"or t:IsA"Explosion"then a:LockProperty(t,"Visible",false)elseif t:IsA"Atmosphere"then a:LockProperty(t,"Density",0)elseif t:IsA"Fire"or t:IsA"ParticleEmitter"or t:IsA"Trail"or t:IsA"SpotLight"or t:IsA"PointLight"or t:IsA"Smoke"or t:IsA"Sparkles"or t:IsA"Beam"or string.find(t.ClassName,"Effect")then a:LockProperty(t,"Enabled",false)end end;do workspace.DescendantAdded:Connect(a.Check)for v,t in next,workspace:GetDescendants()do a.Check(t,true)end;do a:LockProperty(b,"WaterWaveSize",0)a:LockProperty(b,"WaterWaveSpeed",0)a:LockProperty(b,"WaterReflectance",0)a:LockProperty(b,"WaterTransparency",0)end end;do c.ChildAdded:Connect(a.Check)for v,t in next,c:GetChildren()do a.Check(t,true)end;a:LockProperty(c,"GlobalShadows",false)a:LockProperty(c,"FogEnd",9e9)end end else for n,q in next,d do for o,m in next,q do if not m.CantDisable then n[o]=m.OldValue end end end end;g=s end end)return a
