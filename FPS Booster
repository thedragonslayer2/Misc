repeat wait()until game:IsLoaded()if getgenv().FPSBooster then return getgenv().FPSBooster end;local a,b,c,d,e={Enabled=false},workspace:WaitForChild"Terrain",game:GetService"Lighting",getgenv().sethiddenproperty,false;local f,g="MrJackFPSBooster_"pcall(function()g=getgenv().gethiddenproperty(b,"Decoration")end)function a:Toggle(h)a.Enabled=h end;function a:RemoveObj(i,j)i:SetAttribute(f.."LockProperty_"..j,true)i[j]=i:GetAttribute(f..j.."_OldValue")or i[j]end;function a:LockProperty(i,j,k)if i:GetAttribute(f.."LockProperty_"..j)then return end;if not i:GetAttribute(f..j.."_OldValue")then i:SetAttribute(f..j.."_OldValue",i[j])i:GetPropertyChangedSignal(j):Connect(function()if i[j]~=k and(typeof(i[j])~="EnumItem"or i[j].Name~=k)then i:SetAttribute(f..j.."_OldValue",i[j])end;if a.Enabled and not i:GetAttribute(f.."LockProperty_"..j)then i[j]=k end end)end;if not a.Enabled then i[j]=i:GetAttribute(f..j.."_OldValue")else i[j]=k end end;LooP(function()local l=a.Enabled;if e~=l then if d and g then d(b,"Decoration",not l)end;if not a.Check then a.Check=function(m,n)if not n then task.wait()end;if m:IsA"BasePart"then a:LockProperty(m,"Material","Plastic")a:LockProperty(m,"Reflectance",0)elseif m:IsA"ForceField"or m:IsA"Explosion"then a:LockProperty(m,"Visible",false)elseif m:IsA"Atmosphere"then a:LockProperty(m,"Density",0)elseif m:IsA"Fire"or m:IsA"ParticleEmitter"or m:IsA"Trail"or m:IsA"SpotLight"or m:IsA"PointLight"or m:IsA"Smoke"or m:IsA"Sparkles"or m:IsA"Beam"or string.find(m.ClassName,"Effect")then a:LockProperty(m,"Enabled",false)end end;workspace.DescendantAdded:Connect(a.Check)c.ChildAdded:Connect(a.Check)end;do for o,m in next,workspace:GetDescendants()do a.Check(m,true)end;do a:LockProperty(b,"WaterWaveSize",0)a:LockProperty(b,"WaterWaveSpeed",0)a:LockProperty(b,"WaterReflectance",0)a:LockProperty(b,"WaterTransparency",0)end end;do for o,m in next,c:GetChildren()do a.Check(m,true)end;a:LockProperty(c,"GlobalShadows",false)a:LockProperty(c,"FogEnd",9e9)end;e=l en