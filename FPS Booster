repeat wait()until game:IsLoaded()local a,b,c,d,e,f={Disabled=true},workspace:WaitForChild"Terrain",game:GetService"Lighting",{},{},getgenv().sethiddenproperty;if hookmetamethod then local g;g=hookmetamethod(game,"__newindex",function(h,i,j)local k=d[h]and d[h][i]if k and(not a.Disabled or k.CantDisable)then if checkcaller and not checkcaller()then k.OldValue=j end;j=k.Value end;return g(h,i,j)end)end;function a:DontChange(k,l)e[k]=l end;function a:LockProperty(k,l,j,m)if e[k]==l then return end;if not d[k]then d[k]={}end;d[k][l]={Value=j,OldValue=k[l],CantDisable=m}if not a.Disabled or m then k[l]=j end end;function a:Boost()a.Disabled=nil;if f then f(b,"Decoration",false)end;if a.Done then for k,n in next,d do for l,o in next,n do k[l]=o.Value end end;return else a.Done=true;do local function p(q,r)if q:IsA"BasePart"then a:LockProperty(q,"Material","Plastic")a:LockProperty(q,"Reflectance",0)elseif q:IsA"ForceField"or q:IsA"Explosion"then a:LockProperty(q,"Visible",false)elseif q:IsA"Fire"or q:IsA"ParticleEmitter"or q:IsA"Trail"or q:IsA"SpotLight"or q:IsA"PointLight"or q:IsA"Smoke"or q:IsA"Sparkles"or q:IsA"Beam"then a:LockProperty(q,"Enabled",false)end end;workspace.DescendantAdded:Connect(p)for s,q in next,workspace:GetDescendants()do p(q)end end;do a:LockProperty(b,"WaterWaveSize",0)a:LockProperty(b,"WaterWaveSpeed",0)a:LockProperty(b,"WaterReflectance",0)a:LockProperty(b,"WaterTransparency",0)end;do local function p(q,t)if not t then task.wait()end;if string.find(q.ClassName,"Effect")then a:LockProperty(q,"Enabled",false)elseif q:IsA"Atmosphere"then a:LockProperty(q,"Density",0)end end;c.ChildAdded:Connect(p)for s,q in next,c:GetChildren()do p(q,true)end;a:LockProperty(c,"GlobalShadows",false)a:LockProperty(c,"FogEnd",9e9)end end end;function a:Disable()a.Disabled=true;for k,n in next,d do for l,o in next,n do if not o.CantDisable then k[l]=o.OldValue end end end;if f then f(b,"Decoration",true)end end;return a
