repeat wait()until game:IsLoaded()local a,b,c,d,e,f,g={Enabled=false},workspace:WaitForChild"Terrain",game:GetService"Lighting",{},{},getgenv().sethiddenproperty,false;local h=getgenv().gethiddenproperty and getgenv().gethiddenproperty(b,"Decoration")if hookmetamethod then local i;i=hookmetamethod(game,"__newindex",function(j,k,l)local m=d[j.ClassName]and d[j.ClassName][j]and d[j.ClassName][j][k]if m and(a.Enabled or m.CantDisable)then if checkcaller and not checkcaller()then m.OldValue=l end;l=m.Value end;return i(j,k,l)end)end;function a:DontChange(n,o)e[n]=o end;function a:LockProperty(n,o,l,p)if e[n]==o then return end;d[n.ClassName]=d[n.ClassName]or{}d[n.ClassName][n]=d[n.ClassName][n]or{}d[n.ClassName][n][o]={Value=l,OldValue=n[o],CantDisable=p}if a.Enabled or p then n[o]=l end;n.AncestryChanged:Connect(function(q,r)if r==nil then d[n.ClassName][n]=nil end end)end;function a:RemoveObj(n)local s=d[n.ClassName][n]or{}d[n.ClassName][n]=nil;for n,t in next,s do for o,m in next,t do n[o]=m.OldValue end end end;function a:Toggle(u)a.Enabled=u end;LooP(function()local v=a.Enabled;if g~=v then if f and h then f(b,"Decoration",not v)end;if v then if a.Check then for w,x in next,d do for n,t in next,x do for o,m in next,t do n[o]=m.Value end end end else a.Check=function(y,z)if not z then task.wait()end;if y:IsA"BasePart"then a:LockProperty(y,"Material","Plastic")a:LockProperty(y,"Reflectance",0)elseif y:IsA"ForceField"or y:IsA"Explosion"then a:LockProperty(y,"Visible",false)elseif y:IsA"Atmosphere"then a:LockProperty(y,"Density",0)elseif y:IsA"Fire"or y:IsA"ParticleEmitter"or y:IsA"Trail"or y:IsA"SpotLight"or y:IsA"PointLight"or y:IsA"Smoke"or y:IsA"Sparkles"or y:IsA"Beam"or string.find(y.ClassName,"Effect")then a:LockProperty(y,"Enabled",false)end end;do workspace.DescendantAdded:Connect(a.Check)for A,y in next,workspace:GetDescendants()do a.Check(y,true)end;do a:LockProperty(b,"WaterWaveSize",0)a:LockProperty(b,"WaterWaveSpeed",0)a:LockProperty(b,"WaterReflectance",0)a:LockProperty(b,"WaterTransparency",0)end end;do c.ChildAdded:Connect(a.Check)for A,y in next,c:GetChildren()do a.Check(y,true)end;a:LockProperty(c,"GlobalShadows",false)a:LockProperty(c,"FogEnd",9e9)end end else for w,x in next,d do for n,t in next,x do for o,m in next,t do if not m.CantDisable then n[o]=m.OldValue end end end end end;g=v end end)return a
