repeat wait()until game:IsLoaded()local a,b,c,d,e,f={Disabled=true},workspace:WaitForChild"Terrain",game:GetService"Lighting",{},{},getgenv().sethiddenproperty;if hookmetamethod then local g;g=hookmetamethod(game,"__newindex",function(h,i,j)local k=d[h]and d[h][i]if k and(not a.Disabled or k.CantDisable)then if checkcaller and not checkcaller()then k.OldValue=j end;j=k.Value end;return g(h,i,j)end)end;function a:DontChange(k,l)e[k]=l end;function a:LockProperty(k,l,j,m)if e[k]==l then return end;if not d[k]then d[k]={}end;d[k][l]={Value=j,OldValue=k[l],CantDisable=m}if not a.Disabled or m then k[l]=j end end;function a:Boost()a.Disabled=nil;if f then f(b,"Decoration",false)end;if a.Done then for k,n in next,d do k[n.Property]=n.Value end;return else a.Done=true;do local function o(p,q)if p:IsA"BasePart"then a:LockProperty(p,"Material","Plastic")a:LockProperty(p,"Reflectance",0)elseif p:IsA"ForceField"or p:IsA"Explosion"then a:LockProperty(p,"Visible",false)elseif p:IsA"Fire"or p:IsA"ParticleEmitter"or p:IsA"Trail"or p:IsA"SpotLight"or p:IsA"PointLight"or p:IsA"Smoke"or p:IsA"Sparkles"or p:IsA"Beam"then a:LockProperty(p,"Enabled",false)end end;workspace.DescendantAdded:Connect(o)for r,p in next,workspace:GetDescendants()do o(p)end end;do a:LockProperty(b,"WaterWaveSize",0)a:LockProperty(b,"WaterWaveSpeed",0)a:LockProperty(b,"WaterReflectance",0)a:LockProperty(b,"WaterTransparency",0)end;do local function o(p,s)if not s then task.wait()end;if string.find(p.ClassName,"Effect")then a:LockProperty(p,"Enabled",false)elseif p:IsA"Atmosphere"then a:LockProperty(p,"Density",0)end end;c.ChildAdded:Connect(o)for r,p in next,c:GetChildren()do o(p,true)end;a:LockProperty(c,"GlobalShadows",false)a:LockProperty(c,"FogEnd",9e9)end end end;function a:Disable()a.Disabled=true;for k,t in next,d do for l,n in next,t do if not n.CantDisable then k[l]=n.OldValue end end end;if f then f(b,"Decoration",true)end end;return a
