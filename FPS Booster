repeat wait()until game:IsLoaded()local a,b,c,d,e,f={Enabled=false},workspace:WaitForChild"Terrain",game:GetService"Lighting",{},getgenv().sethiddenproperty,false;local g=getgenv().gethiddenproperty and getgenv().gethiddenproperty(b,"Decoration")function a:LockProperty(h,i,j)d[h.ClassName]=d[h.ClassName]or{}d[h.ClassName]=d[h.ClassName]or{}d[h.ClassName][h]=d[h.ClassName][h]or{}d[h.ClassName][h][i]={Value=j,OldValue=h[i]}h:GetPropertyChangedSignal(i):Connect(function()if h[i]~=j and(typeof(h[i])~="EnumItem"or h[i].Name~=j)then d[h.ClassName][h][i].OldValue=h[i]end;if a.Enabled then h[i]=j end end)if a.Enabled then h[i]=j end end;function a:RemoveObj(h)local k=d[h.ClassName][h]d[h.ClassName][h]=nil;for i,l in next,k or{}do h[i]=l.OldValue end end;function a:Toggle(m)a.Enabled=m end;LooP(function()local n=a.Enabled;if f~=n then if e and g then e(b,"Decoration",not n)end;if n then if a.Check then for o,p in next,d do for h,q in next,p do for i,l in next,q do h[i]=l.Value end end end else a.Check=function(r,s)if not s then task.wait()end;if r:IsA"BasePart"then a:LockProperty(r,"Material","Plastic")a:LockProperty(r,"Reflectance",0)elseif r:IsA"ForceField"or r:IsA"Explosion"then a:LockProperty(r,"Visible",false)elseif r:IsA"Atmosphere"then a:LockProperty(r,"Density",0)elseif r:IsA"Fire"or r:IsA"ParticleEmitter"or r:IsA"Trail"or r:IsA"SpotLight"or r:IsA"PointLight"or r:IsA"Smoke"or r:IsA"Sparkles"or r:IsA"Beam"or string.find(r.ClassName,"Effect")then a:LockProperty(r,"Enabled",false)end end;do workspace.DescendantAdded:Connect(a.Check)for t,r in next,workspace:GetDescendants()do a.Check(r,true)end;do a:LockProperty(b,"WaterWaveSize",0)a:LockProperty(b,"WaterWaveSpeed",0)a:LockProperty(b,"WaterReflectance",0)a:LockProperty(b,"WaterTransparency",0)end end;do c.ChildAdded:Connect(a.Check)for t,r in next,c:GetChildren()do a.Check(r,true)end;a:LockProperty(c,"GlobalShadows",false)a:LockProperty(c,"FogEnd",9e9)end end else for o,p in next,d do for h,q in next,p do for i,l in next,q do h[i]=l.OldValue end end end end;f=n end end)return a
