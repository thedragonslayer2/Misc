repeat wait()until game:IsLoaded()local a,b,c,d,e,f,g,h={Enabled=false},workspace:WaitForChild"Terrain",game:GetService"Lighting",{},{},{},getgenv().sethiddenproperty,false;local i=getgenv().gethiddenproperty and getgenv().gethiddenproperty(b,"Decoration")if hookmetamethod then local j;j=hookmetamethod(game,"__newindex",function(k,l,m)local n=d[k]and d[k][l]if n and(a.Enabled or n.CantDisable)then if checkcaller and not checkcaller()then n.OldValue=m end;if not f[k]then m=n.Value end end;return j(k,l,m)end)end;function a:DontChange(o,p)e[o]=p end;function a:RemoveObj(o)f[o]=true end;function a:LockProperty(o,p,m,q)if e[o]==p then return end;if not d[o]then d[o]={}end;d[o][p]={Value=m,OldValue=o[p],CantDisable=q}if a.Enabled or q then o[p]=m end end;function a:Toggle(r)a.Enabled=r end;LooP(function()local s=a.Enabled;if h~=s then if g and i then g(b,"Decoration",not s)end;if s then if a.Check then for o,t in next,d do for p,n in next,t do o[p]=n.Value end end else a.Check=function(u,v)if not v then task.wait()end;if u:IsA"BasePart"then a:LockProperty(u,"Material","Plastic")a:LockProperty(u,"Reflectance",0)elseif u:IsA"ForceField"or u:IsA"Explosion"then a:LockProperty(u,"Visible",false)elseif u:IsA"Atmosphere"then a:LockProperty(u,"Density",0)elseif u:IsA"Fire"or u:IsA"ParticleEmitter"or u:IsA"Trail"or u:IsA"SpotLight"or u:IsA"PointLight"or u:IsA"Smoke"or u:IsA"Sparkles"or u:IsA"Beam"or string.find(u.ClassName,"Effect")then a:LockProperty(u,"Enabled",false)end end;do workspace.DescendantAdded:Connect(a.Check)for w,u in next,workspace:GetDescendants()do a.Check(u,true)end;do a:LockProperty(b,"WaterWaveSize",0)a:LockProperty(b,"WaterWaveSpeed",0)a:LockProperty(b,"WaterReflectance",0)a:LockProperty(b,"WaterTransparency",0)end end;do c.ChildAdded:Connect(a.Check)for w,u in next,c:GetChildren()do a.Check(u,true)end;a:LockProperty(c,"GlobalShadows",false)a:LockProperty(c,"FogEnd",9e9)end end else for o,t in next,d do for p,n in next,t do if not n.CantDisable then o[p]=n.OldValue end end end end;h=s end end)return a
