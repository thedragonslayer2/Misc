repeat wait()until game:IsLoaded()local a,b,c,d,e,f={Disabled=true},workspace:WaitForChild"Terrain",game:GetService"Lighting",{},{},getgenv().sethiddenproperty;if hookmetamethod then local g;g=hookmetamethod(game,"__newindex",function(h,i,j)local k=d[h]and d[h][i]if k and(not a.Disabled or k.CantDisable)then if checkcaller and not checkcaller()then k.OldValue=j end;j=k.Value end;return g(h,i,j)end)end;function a:DontChange(k,l)e[k]=l end;function a:LockProperty(k,l,j,m)if e[k]==l then return end;if not d[k]then d[k]={}end;d[k][l]={Value=j,OldValue=k[l],CantDisable=m}if not a.Disabled or m then k[l]=j end end;function a:Toggle(n)if a.Busy then return end;a.Busy,a.Disabled=true,not n;if f then f(b,"Decoration",not n)end;if n then if a.Done then for k,o in next,d do for l,p in next,o do k[l]=p.Value end end;return else a.Done=true;do local function q(r,s)if r:IsA"BasePart"then a:LockProperty(r,"Material","Plastic")a:LockProperty(r,"Reflectance",0)elseif r:IsA"ForceField"or r:IsA"Explosion"then a:LockProperty(r,"Visible",false)elseif r:IsA"Fire"or r:IsA"ParticleEmitter"or r:IsA"Trail"or r:IsA"SpotLight"or r:IsA"PointLight"or r:IsA"Smoke"or r:IsA"Sparkles"or r:IsA"Beam"then a:LockProperty(r,"Enabled",false)end end;workspace.DescendantAdded:Connect(q)for t,r in next,workspace:GetDescendants()do q(r)end end;do a:LockProperty(b,"WaterWaveSize",0)a:LockProperty(b,"WaterWaveSpeed",0)a:LockProperty(b,"WaterReflectance",0)a:LockProperty(b,"WaterTransparency",0)end;do local function q(r,u)if not u then task.wait()end;if string.find(r.ClassName,"Effect")then a:LockProperty(r,"Enabled",false)elseif r:IsA"Atmosphere"then a:LockProperty(r,"Density",0)end end;c.ChildAdded:Connect(q)for t,r in next,c:GetChildren()do q(r,true)end;a:LockProperty(c,"GlobalShadows",false)a:LockProperty(c,"FogEnd",9e9)end end else for k,o in next,d do for l,p in next,o do if not p.CantDisable then k[l]=p.OldValue end end end end;a.Busy=nil end;return a
