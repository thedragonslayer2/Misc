repeat wait()until game:IsLoaded()local a,b,c,d,e,f={Enabled=false},workspace:WaitForChild"Terrain",game:GetService"Lighting",{},{},getgenv().sethiddenproperty;local g=getgenv().gethiddenproperty and getgenv().gethiddenproperty(b,"Decoration")if hookmetamethod then local h;h=hookmetamethod(game,"__newindex",function(i,j,k)local l=d[i]and d[i][j]if l and(a.Enabled or l.CantDisable)then if checkcaller and not checkcaller()then l.OldValue=k end;k=l.Value end;return h(i,j,k)end)end;function a:DontChange(l,m)e[l]=m end;function a:LockProperty(l,m,k,n)if e[l]==m then return end;if not d[l]then d[l]={}end;d[l][m]={Value=k,OldValue=l[m],CantDisable=n}if a.Enabled or n then l[m]=k end end;function a:Toggle(o)a.Enabled=o end;task.spawn(function()local p=false;while task.wait()do local q=a.Enabled;if p~=q then if f and g then f(b,"Decoration",not q)end;if q then if a.Check then for l,r in next,d do for m,s in next,r do l[m]=s.Value end end else a.Check=function(t,u)if not u then task.wait()end;if t:IsA"BasePart"then a:LockProperty(t,"Material","Plastic")a:LockProperty(t,"Reflectance",0)elseif t:IsA"ForceField"or t:IsA"Explosion"then a:LockProperty(t,"Visible",false)elseif t:IsA"Atmosphere"then a:LockProperty(t,"Density",0)elseif t:IsA"Fire"or t:IsA"ParticleEmitter"or t:IsA"Trail"or t:IsA"SpotLight"or t:IsA"PointLight"or t:IsA"Smoke"or t:IsA"Sparkles"or t:IsA"Beam"or string.find(t.ClassName,"Effect")then a:LockProperty(t,"Enabled",false)end end;do workspace.DescendantAdded:Connect(a.Check)for v,t in next,workspace:GetDescendants()do a.Check(t,true)end;do a:LockProperty(b,"WaterWaveSize",0)a:LockProperty(b,"WaterWaveSpeed",0)a:LockProperty(b,"WaterReflectance",0)a:LockProperty(b,"WaterTransparency",0)end end;do c.ChildAdded:Connect(a.Check)for v,t in next,c:GetChildren()do a.Check(t,true)end;a:LockProperty(c,"GlobalShadows",false)a:LockProperty(c,"FogEnd",9e9)end end else for l,r in next,d do for m,s in next,r do if not s.CantDisable then l[m]=s.OldValue end end end end;p=q end end end)return a
