repeat wait()until game:IsLoaded()local a,b,c,d,e={Enabled=false},workspace:WaitForChild"Terrain",game:GetService"Lighting",{},getgenv().sethiddenproperty;local f=getgenv().gethiddenproperty and getgenv().gethiddenproperty(b,"Decoration")local function g(h,i,j)if d[h]==i or not a.Enabled then return end;local k,l=h[i]l=h:GetPropertyChangedSignal(i):Connect(function()if h[i]~=j and(typeof(h[i])~="EnumItem"or h[i].Name~=j)then k=h[i]end;if a.Enabled then h[i]=j end end)h[i]=j;repeat task.wait()until not a.Enabled or d[h]==i;h[i],k,l=k end;a.Check=function(m,n)if not n then task.wait(0.1)end;if m:IsA"BasePart"then a:LockProperty(m,"Material","Plastic")a:LockProperty(m,"Reflectance",0)elseif m:IsA"ForceField"or m:IsA"Explosion"then a:LockProperty(m,"Visible",false)elseif m:IsA"Atmosphere"then a:LockProperty(m,"Density",0)elseif m:IsA"Fire"or m:IsA"ParticleEmitter"or m:IsA"Trail"or m:IsA"SpotLight"or m:IsA"PointLight"or m:IsA"Smoke"or m:IsA"Sparkles"or m:IsA"Beam"or string.find(m.ClassName,"Effect")then a:LockProperty(m,"Enabled",false)end end;function a:RemoveObj(h,i)d[h]=i end;function a:LockProperty(h,i,j)task.defer(g,h,i,j)end;function a:Toggle(o)a.Enabled=o;if not o then return end;if e and f then e(b,"Decoration",not o)end;do for p,m in next,workspace:GetDescendants()do a.Check(m,true)end;do a:LockProperty(b,"WaterWaveSize",0)a:LockProperty(b,"WaterWaveSpeed",0)a:LockProperty(b,"WaterReflectance",0)a:LockProperty(b,"WaterTransparency",0)end end;do for p,m in next,c:GetChildren()do a.Check(m,true)end;a:LockProperty(c,"GlobalShadows",false)a:LockProperty(c,"FogEnd",9e9)end end;workspace.DescendantAdded:Connect(a.Check)c.ChildAdded:Connect(a.Check)return a
