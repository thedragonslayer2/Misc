repeat wait()until game:IsLoaded()local a,b,c,d,e,f={Enabled=false},workspace:WaitForChild"Terrain",game:GetService"Lighting",{},{},getgenv().sethiddenproperty;local g=getgenv().gethiddenproperty and getgenv().gethiddenproperty(b,"Decoration")if hookmetamethod then local h;h=hookmetamethod(game,"__newindex",function(i,j,k)local l=d[i]and d[i][j]if l and(a.Enabled or l.CantDisable)then if checkcaller and not checkcaller()then l.OldValue=k end;k=l.Value end;return h(i,j,k)end)end;function a:DontChange(l,m)e[l]=m end;function a:LockProperty(l,m,k,n)if e[l]==m then return end;if not d[l]then d[l]={}end;d[l][m]={Value=k,OldValue=l[m],CantDisable=n}if a.Enabled or n then l[m]=k end end;function a:Toggle(o)a.Enabled=o end;task.spawn(function()local p=false;while task.wait()do local q=a.Enabled;if p~=q then if f and g then f(b,"Decoration",not q)end;if q then for l,r in next,d do for m,s in next,r do if not s.CantDisable then l[m]=s.OldValue end end end else if a.Done then for l,r in next,d do for m,s in next,r do l[m]=s.Value end end else a.Done=true;do local function t(u,v)if u:IsA"BasePart"then a:LockProperty(u,"Material","Plastic")a:LockProperty(u,"Reflectance",0)elseif u:IsA"ForceField"or u:IsA"Explosion"then a:LockProperty(u,"Visible",false)elseif u:IsA"Fire"or u:IsA"ParticleEmitter"or u:IsA"Trail"or u:IsA"SpotLight"or u:IsA"PointLight"or u:IsA"Smoke"or u:IsA"Sparkles"or u:IsA"Beam"then a:LockProperty(u,"Enabled",false)end end;workspace.DescendantAdded:Connect(t)for w,u in next,workspace:GetDescendants()do t(u)end end;do a:LockProperty(b,"WaterWaveSize",0)a:LockProperty(b,"WaterWaveSpeed",0)a:LockProperty(b,"WaterReflectance",0)a:LockProperty(b,"WaterTransparency",0)end;do local function t(u,x)if not x then task.wait()end;if string.find(u.ClassName,"Effect")then a:LockProperty(u,"Enabled",false)elseif u:IsA"Atmosphere"then a:LockProperty(u,"Density",0)end end;c.ChildAdded:Connect(t)for w,u in next,c:GetChildren()do t(u,true)end;a:LockProperty(c,"GlobalShadows",false)a:LockProperty(c,"FogEnd",9e9)end end end;p=a.Enabled end end end)return a
