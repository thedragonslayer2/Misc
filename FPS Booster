repeat wait()until game:IsLoaded()local a={DCH={}}local function b(c,d)if not d then task.wait()end;if c:IsA"BasePart"then a:LockProperty(c,"Material","Plastic")a:LockProperty(c,"Reflectance",0)elseif c:IsA"ForceField"or c:IsA"Explosion"then a:LockProperty(c,"Visible",false)elseif c:IsA"Atmosphere"then a:LockProperty(c,"Density",0)elseif c:IsA"Fire"or c:IsA"ParticleEmitter"or c:IsA"Trail"or c:IsA"SpotLight"or c:IsA"PointLight"or c:IsA"Smoke"or c:IsA"Sparkles"or c:IsA"Beam"or string.find(c.ClassName,"Effect")then a:LockProperty(c,"Enabled",false)end end;function a:DontChange(e,f)a.DCH[e]=f end;function a:LockProperty(e,f,g)local h=e[f]e:GetPropertyChangedSignal(f):Connect(function()if not a.Enabled or e[f]~=g then h=e[f]end end)task.spawn(function()while task.wait()do if a.Enabled and not a.DCH[e]then if e[f]~=g then e[f]=g end elseif e[f]~=h then e[f]=h end end end)end;do local i,j=workspace:WaitForChild"Terrain",game:GetService"Lighting"local k,l=getgenv().gethiddenproperty and getgenv().gethiddenproperty(i,"Decoration"),getgenv().sethiddenproperty;do workspace.DescendantAdded:Connect(b)for m,c in next,workspace:GetDescendants()do b(c,true)end;do a:LockProperty(i,"WaterWaveSize",0)a:LockProperty(i,"WaterWaveSpeed",0)a:LockProperty(i,"WaterReflectance",0)a:LockProperty(i,"WaterTransparency",1)if l and k then task.spawn(function()while task.wait()do if l and k then l(i,"Decoration",not a.Enabled)end end end)end end end;do j.ChildAdded:Connect(b)for m,c in next,j:GetChildren()do b(c,true)end;a:LockProperty(j,"GlobalShadows",false)a:LockProperty(j,"FogEnd",9e9)end end;function a:Toggle(n)a.Enabled=n end;return a
