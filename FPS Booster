repeat wait()until game:IsLoaded()local a,b,c,d,e,f,g={Enabled=false},workspace:WaitForChild"Terrain",game:GetService"Lighting",{},{},getgenv().sethiddenproperty,false;local h=getgenv().gethiddenproperty and getgenv().gethiddenproperty(b,"Decoration")if hookmetamethod then local i;i=hookmetamethod(game,"__newindex",function(j,k,l)local m=d[j]and d[j][k]if m and(a.Enabled or m.CantDisable)then if checkcaller and not checkcaller()then m.OldValue=l end;l=m.Value end;return i(j,k,l)end)end;function a:DontChange(m,n)e[m]=n end;function a:LockProperty(m,n,l,o)if e[m]==n then return end;if not d[m]then d[m]={}end;d[m][n]={Value=l,OldValue=m[n],CantDisable=o}if a.Enabled or o then m[n]=l end end;function a:Toggle(p)a.Enabled=p end;LooP(function()local q=a.Enabled;if g~=q then if f and h then f(b,"Decoration",not q)end;if q then if a.Check then for m,r in next,d do for n,s in next,r do m[n]=s.Value end end else a.Check=function(t,u)if not u then task.wait()end;if t:IsA"BasePart"then a:LockProperty(t,"Material","Plastic")a:LockProperty(t,"Reflectance",0)elseif t:IsA"ForceField"or t:IsA"Explosion"then a:LockProperty(t,"Visible",false)elseif t:IsA"Atmosphere"then a:LockProperty(t,"Density",0)elseif t:IsA"Fire"or t:IsA"ParticleEmitter"or t:IsA"Trail"or t:IsA"SpotLight"or t:IsA"PointLight"or t:IsA"Smoke"or t:IsA"Sparkles"or t:IsA"Beam"or string.find(t.ClassName,"Effect")then a:LockProperty(t,"Enabled",false)end end;do workspace.DescendantAdded:Connect(a.Check)for v,t in next,workspace:GetDescendants()do a.Check(t,true)end;do a:LockProperty(b,"WaterWaveSize",0)a:LockProperty(b,"WaterWaveSpeed",0)a:LockProperty(b,"WaterReflectance",0)a:LockProperty(b,"WaterTransparency",0)end end;do c.ChildAdded:Connect(a.Check)for v,t in next,c:GetChildren()do a.Check(t,true)end;a:LockProperty(c,"GlobalShadows",false)a:LockProperty(c,"FogEnd",9e9)end end else for m,r in next,d do for n,s in next,r do if not s.CantDisable then m[n]=s.OldValue end end end end;g=q end end)return a
